    volume = {40},
    %url = {http://www.thomistica.info/bth/index.php?action=resourceView&id=68853&access=1&search=highlight},
    abstract = {Does Aquinas recognize two human ends, a natural and a supernatural end, or does he recognize only one human end, the supernatural end or Beatific Vision? The answer is that he gets between abandoning outright the natural human end and simply adding that end onto the supernatural end without integrating the two. Instead, he weaves the two ends together. He makes one element in the natural end, i. e., acting well, a necessary means to the vision. Then he identifies the vision with the quintessence of the other element in the natural human end, i. e., thinking well or excellently. He thus avoids the extremes of intellectualism and pragmatism in ethics},
    %number = {3},
    journal = {Aquinas},
    author = {Peterson, J.},
    year = {1997},
    pages = {429--440},
    pagination = {none},
}

