@article{bastit_m_revue_thomiste,
    title = {Le thomisme est-il un aristotélisme?},
    volume = {101},
    abstract = {A la différence des interprétations devenues classiques, on soutient ici que la structure fondamentale de la pensée de saint Thomas est un aristotélisme qui lui permet d'assimiler et de dominer les éléments néo-platoniciens que l'on découvre chez lui. Après vérification de la thèse successivement a propos de la diversité de l'être, de la hiérarchie formelle, et enfin de la composition d'essence et d'esse, on suggère qu'il ne peut en être autrement dans une pensée théologique qui se doit de reconnaître et la transcendance du Premier des êtres, et la consistance d'une nature qui est son oeuvre.},
    %number = {1},
    journal = {Revue Thomiste},
    author = {Bastit, M.},
    year = {2001},
    pages = {101--116},
    pagination = {none},
}

@article{beuchot_puente_m_ciencia_tomista,
    title = {`Phrónesis', hermenéutica y símbolo},
    volume = {133},
    %url = {http://www.thomistica.info/bth/index.php?action=resourceView&id=83783&access=1&search=highlight},
    abstract = {Quiere conectarse en este capítulo la razón práctica, la phrónesis y la hermenéutica, pues uno de los ejemplos de la razón práctica que habitualmente se toman (desde Aristóteles) es el de la prudencia, a la que se le ha asignado un papel nuclear, la llave de las virtudes. Hace un repaso por la concepción antigua y medieval de la prudencia, para compararla con el valor que se le da en el pensamiento moderno y contemporáneo (que ha intentado recuperar el valor de la razón práctica, en parte como salida de la bancarrota de la razón moderna). Para esta virtud es importante el valor de la analogía frente a la univocidad.
Summary Author: María Idoya Zorroza, Universidad de Navarra, Pamplona},
    %number = {429},
    journal = {Ciencia Tomista},
    author = {Beuchot Puente, M.},
    year = {2006},
    pages = {5--14},
    pagination = {none},
}

@incollection{bourke_nicomachean_1974,
    address = {Toronto},
    title = {The `Nicomachean Ethics' and Thomas Aquinas.},
    volume = {1},
    %url = {http://www.thomistica.info/bth/index.php?action=resourceView&id=61749&access=1&search=highlight},
    abstract = {The purpose is to show the relation of Aquinas' moral Philosophy to aristotle's. Based on the new (1969) edition of Thomas' commentary and the recent literature on it, this article studies the text of aristotle used by Aquinas, prior commentaries used, date and place of writing his commentary, its relation to his other ethical writings, the extent to which Aquinas modified aristotle, and the ethical value of the commentary. The conclusion is that the commentary is an important part of Aquinas' ethics.},
    booktitle = {St. Thomas Aquinas 1274-1974: Commemorative Studies},
    publisher = {Pontifical Institute of Mediaeval Studies},
    author = {Bourke, V. J.},
    year = {1974},
    pages = {239--259},
